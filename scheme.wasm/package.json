{
  "name": "scheme.wasm",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/src/index.js",
  "bin": {
    "scheme.wasm": "./dist/src/index.js",
    "buildwat": "./dist/src/scripts/buildwat.js",
    "unicodedata": "./dist/src/scripts/unicodedata.js"
  },
  "scripts": {
    "start-dev": "nodemon --watch src -e ts --exec \"npm run start\"",
    "start": "node .",
    "prestart": "npm run build",
    "build": "tsc --incremental -p . && npm run unicode && npm run assemble",
    "assemble": "node dist/src/scripts/buildwat.js ./buildwat.json --macros --output dist/scheme.wasm --tempfile dist/scheme.wat",
    "pretest": "tsc --incremental -p . && npm run unicode && node dist/src/scripts/buildwat.js tests/buildwat.json --macros --output dist/test.wasm --tempfile dist/test.wat",
    "unicode": "node dist/src/scripts/unicodedata.js",
    "test": "node ../node_modules/mocha/bin/_mocha -r ts-node/register --no-timeouts tests/**/*.spec.ts"
  },
  "keywords": [],
  "author": "Paul C Roberts <pacaro@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "chai": "^4.3.4",
    "commander": "^8.3.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "wabt": "^1.0.24"
  }
}